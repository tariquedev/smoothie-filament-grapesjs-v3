{
  "name": "tarique/smoothie-filament-grapesjs-v3",
  "description": "",
  "keywords": [
    "tarique",
    "laravel",
    "filament-grapesjs-v3"
  ],
  "homepage": "https://github.com/tariquedev/smoothie-filament-grapesjs-v3",
  "support": {
    "issues": "https://github.com/tariquedev/smoothie-filament-grapesjs-v3/issues",
    "source": "https://github.com/tariquedev/smoothie-filament-grapesjs-v3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Tarique Hasan",
      "email": "tariquehasan19@gmail.com",
      "role": "Full Stack Engineer"
    }
  ],
  "require": {
    "php": "^8.1",
    "filament/filament": "^3.0",
    "spatie/laravel-package-tools": "^1.15.0",
    "illuminate/contracts": "^10.0 || ^11.0"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^7.9",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^8.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-arch": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Dotswan\\FilamentGrapesjs\\": "src/",
      "Dotswan\\FilamentGrapesjs\\Database\\Factories\\": "database/factories/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dotswan\\FilamentGrapesjs\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tarique\\FilamentGrapesjs\\FilamentGrapesJsServiceProvider"
      ],
      "aliases": {
        "FilamentGrapesjs": "Tarique\\FilamentGrapesjs\\Facades\\FilamentGrapesjs"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
